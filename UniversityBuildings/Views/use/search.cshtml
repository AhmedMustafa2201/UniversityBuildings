@model IEnumerable<UniversityBuildings.Models.Faculty_Buildings>
@{ 
    Layout = "~/Views/Shared/_Master.cshtml";
    var currentlg = ViewBag.currentLanguage;
    if (currentlg=="en")
    {
        ViewBag.Title = "Search";
    }
    else
    {
        ViewBag.Title = "بحث";
    }
}

@using (Ajax.BeginForm(new AjaxOptions()
                            {
                                HttpMethod = "POST",
                                UpdateTargetId = "searchResult",
                                InsertionMode = InsertionMode.Replace
                            }))
{
    
        @Html.AntiForgeryToken()
        <div class="flexbox" style="background-color:#636E72;padding-bottom:.5rem">
            <div class="search">
                <div style="float:right">
                    <input dir="@UniversityBuildings.Resources.Resource.dir" style="font-family:'Droid Arabic Kufi',sans-serif" type="text" placeholder="@UniversityBuildings.Resources.Resource.searchPlace" required="" autocomplete="off" id="pname" name="pname" data-autocomplete="@Url.Action("quickSearch")">
                </div>
            </div>
        </div>
}
<div id="searchResult">

        @Html.Partial("_mainSearch", Model)

</div>

